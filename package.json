    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "prisma": "^3.9.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.1.2"

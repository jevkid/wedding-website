{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "node-sass ./styles -o ./app/styles --output-style compressed",
    "dev:css": "npm run build:css && node-sass -rw ./styles -o ./app/styles",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "postinstall": "remix setup node",
    "pullDB": "npx prisma db pull --force",
    "generate": "npx prisma generate"
  },
  "dependencies": {
    "@dhaiwat10/react-link-preview": "^1.13.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@prisma/client": "^3.9.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "@types/date-fns": "^2.6.0",
    "date-fns": "^2.28.0",
    "firebase": "^9.7.0",
    "framer-motion": "^6.3.3",
    "marked": "^4.0.12",
    "node-sass": "^7.0.1",
    "pigeon-maps": "^0.21.0",
    "postcss": "^8.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/google-map-react": "^2.1.5",
    "@types/google.maps": "^3.48.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "google-map-react": "^2.1.10",
    "prisma": "^3.9.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
